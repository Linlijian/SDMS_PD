@section Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/Chart.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Chart.bundle.min.js")"></script>
    <script>
        $(document).ready(function () {
            $.ajax({
                url: '@Url.Action("Dashboard", "Account", new { Area = "Users"})',
                type: 'post',
                data: null,
                contentType: false,
                processData: false,
                success: function (Dashboard) {
                    if (Dashboard != null && Dashboard != isNullOrEmpty) {
                        //PriolityChart
                        var ctx = document.getElementById('PriolityChart').getContext('2d');
                        var chart = new Chart(ctx, {
                            // The type of chart we want to create
                            type: 'line',

                            // The data for our dataset
                            data: {
                                labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],
                                datasets: [{
                                    fill: false,
                                    label: 'FZMS All Year',
                                    backgroundColor: 'rgb(254, 195, 254)',
                                    borderColor: 'rgb(254, 195, 254)',
                                    data: [ Dashboard.data.MONTH_FZMS_1,
                                            Dashboard.data.MONTH_FZMS_2,
                                            Dashboard.data.MONTH_FZMS_3,
                                            Dashboard.data.MONTH_FZMS_4,
                                            Dashboard.data.MONTH_FZMS_5,
                                            Dashboard.data.MONTH_FZMS_6,
                                            Dashboard.data.MONTH_FZMS_7,
                                            Dashboard.data.MONTH_FZMS_8,
                                            Dashboard.data.MONTH_FZMS_9,
                                            Dashboard.data.MONTH_FZMS_10,
                                            Dashboard.data.MONTH_FZMS_11,
                                            Dashboard.data.MONTH_FZMS_12
                                    ]
                                }, {
                                    fill: false,
                                    label: 'GBMSR2 All Year',
                                    backgroundColor: 'rgb(105, 195, 254)',
                                    borderColor: 'rgb(105, 195, 254)',
                                    data: [ Dashboard.data.MONTH_GBMSR2_1,
                                            Dashboard.data.MONTH_GBMSR2_2,
                                            Dashboard.data.MONTH_GBMSR2_3,
                                            Dashboard.data.MONTH_GBMSR2_4,
                                            Dashboard.data.MONTH_GBMSR2_5,
                                            Dashboard.data.MONTH_GBMSR2_6,
                                            Dashboard.data.MONTH_GBMSR2_7,
                                            Dashboard.data.MONTH_GBMSR2_8,
                                            Dashboard.data.MONTH_GBMSR2_9,
                                            Dashboard.data.MONTH_GBMSR2_10,
                                            Dashboard.data.MONTH_GBMSR2_11,
                                            Dashboard.data.MONTH_GBMSR2_12]
                                }, {
                                    fill: false,
                                    label: 'DLT All Year',
                                    backgroundColor: 'rgb(105, 195, 128)',
                                    borderColor: 'rgb(105, 195, 128)',
                                    data: [Dashboard.data.MONTH_DLT_1,
                                           Dashboard.data.MONTH_DLT_2,
                                           Dashboard.data.MONTH_DLT_3,
                                           Dashboard.data.MONTH_DLT_4,
                                           Dashboard.data.MONTH_DLT_5,
                                           Dashboard.data.MONTH_DLT_6,
                                           Dashboard.data.MONTH_DLT_7,
                                           Dashboard.data.MONTH_DLT_8,
                                           Dashboard.data.MONTH_DLT_9,
                                           Dashboard.data.MONTH_DLT_10,
                                           Dashboard.data.MONTH_DLT_11,
                                           Dashboard.data.MONTH_DLT_12
                                    ]
                                }, {
                                    fill: false,
                                    label: 'EXTS All Year',
                                    backgroundColor: 'rgb(245, 239, 120)',
                                    borderColor: 'rgb(245, 239, 120)',
                                    data: [Dashboard.data.MONTH_EXTS_1,
                                           Dashboard.data.MONTH_EXTS_2,
                                           Dashboard.data.MONTH_EXTS_3,
                                           Dashboard.data.MONTH_EXTS_4,
                                           Dashboard.data.MONTH_EXTS_5,
                                           Dashboard.data.MONTH_EXTS_6,
                                           Dashboard.data.MONTH_EXTS_7,
                                           Dashboard.data.MONTH_EXTS_8,
                                           Dashboard.data.MONTH_EXTS_9,
                                           Dashboard.data.MONTH_EXTS_10,
                                           Dashboard.data.MONTH_EXTS_11,
                                           Dashboard.data.MONTH_EXTS_12
                                    ]
                                }, {
                                    fill: false,
                                    label: 'CMAH All Year',
                                    backgroundColor: 'rgb(187, 30, 254)',
                                    borderColor: 'rgb(187, 30, 254)',
                                    data: [Dashboard.data.MONTH_CMAH_1,
                                           Dashboard.data.MONTH_CMAH_2,
                                           Dashboard.data.MONTH_CMAH_3,
                                           Dashboard.data.MONTH_CMAH_4,
                                           Dashboard.data.MONTH_CMAH_5,
                                           Dashboard.data.MONTH_CMAH_6,
                                           Dashboard.data.MONTH_CMAH_7,
                                           Dashboard.data.MONTH_CMAH_8,
                                           Dashboard.data.MONTH_CMAH_9,
                                           Dashboard.data.MONTH_CMAH_10,
                                           Dashboard.data.MONTH_CMAH_11,
                                           Dashboard.data.MONTH_CMAH_12
                                    ]
                                }

                                ]
                            },

                            // Configuration options go here
                            options: {
                                responsive: true,
                                title: {
                                    display: true,
                                    text: 'Issue on Year'
                                },
                                tooltips: {
                                    mode: 'index',
                                    intersect: false,
                                },
                                hover: {
                                    mode: 'nearest',
                                    intersect: true
                                }, scales: {
                                    xAxes: [{
                                        display: true,
                                        scaleLabel: {
                                            display: true,
                                            labelString: 'Month'
                                        }
                                    }],
                                    yAxes: [{
                                        display: true,
                                        scaleLabel: {
                                            display: true,
                                            labelString: 'Value'
                                        }
                                    }]
                                }
                            }
                        });
                        //
                        window.onload = function () {
                            var ctx = document.getElementById('AllAppLication').getContext('2d');
                            window.myDoughnut = new Chart(ctx, config);
                        };
                        var config = {
                            type: 'doughnut',
                            data: {
                                datasets: [{
                                    data: [
                                       Dashboard.data.APP_1, Dashboard.data.APP_2, Dashboard.data.APP_3, Dashboard.data.APP_4, Dashboard.data.APP_5
                                    ],
                                    backgroundColor: [
                                        'Red',
                                    'Orange',
                                    'Yellow',
                                    'Green',
                                    'Blue'
                                    ],
                                    label: 'Dataset 1'
                                }],
                                labels: [
                                    'FZMS',
                                    'GBMSR2',
                                    'DLT',
                                    'EXTS',
                                    'CMAH'
                                ]
                            },
                            options: {
                                responsive: true,
                                legend: {
                                    position: 'top',
                                },
                                title: {
                                    display: true,
                                    text: 'Issue Opening in All Application'
                                },
                                animation: {
                                    animateScale: true,
                                    animateRotate: true
                                }
                            }
                        };
                    }
                },
                error: OnAjaxError
            });
        });

    </script>
}
