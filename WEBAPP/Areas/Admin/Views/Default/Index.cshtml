}

@section stylesheet{
    <style>
        .btn-primary, .btn-primary:focus {
            background-color: #003c72 !important;
            border-color: #003c72;
        }

        .page-content {
            background-color: #fff;
            position: relative;
            margin: 0;
            padding-left: 0px;
            padding-right: 0px;
        }

        .ash {
            color: #9f9f9f;
        }
    </style>
    <link href="@Url.Content("~/Content/Chart.min.css")" rel="stylesheet" />
}
