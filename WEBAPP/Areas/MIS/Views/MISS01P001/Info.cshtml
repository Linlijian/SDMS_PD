    {
        using (row.GridColumn(12).SetXs(12).Begin())
        {
            using (var r = Html.Bootstrap().GridRow().Begin())
            {
                using (r.GridColumn(6).Begin())
                {
                    <div class="form-group">
                        <label for="@Translation.MIS.MISS01P001.ID_REF_NO" class="control-label col-md-4">@Translation.MIS.MISS01P001.REF_NO :</label>
                        <div class="col-md-8">
                            <label for="@Translation.MIS.MISS01P001.ID_REF_NO" id="@Translation.MIS.MISS01P001.ID_REF_NO" class="control-label">@Model.REF_NO</label>
                        </div>
                    </div>
                }
            }
            using (var r = Html.Bootstrap().GridRow().Begin())
            {
                using (r.GridColumn(6).Begin())
                {
                    <div class="form-group">
                        <label class="control-label col-md-4">@Translation.MIS.MISS01P001.APP_CODE :</label>
                        <div class="col-md-8">
                            <label class="control-label">@Model.COM_CODE</label>
                        </div>
                    </div>
                }
                using (r.GridColumn(6).Begin())
                {
                    <div class="form-group">
                        <label class="control-label col-md-4">@Translation.MIS.MISS01P001.NO :</label>
                        <div class="col-md-8">
                            <label class="control-label">@Model.NO</label>
                        </div>
                    </div>
                }
            }
            using (var r = Html.Bootstrap().GridRow().Begin())
            {
