                        <div class="col-md-8">
                            <label class="control-label">@Model.ISSUE_DATE</label>
                        </div>
                    </div>
                }

                using (r.GridColumn(6).Begin())
                {
                    <div class="form-group">
                        <label class="control-label col-md-4">@Translation.MIS.MISS01P001.ISSUE_DATE_PERIOD :</label>
