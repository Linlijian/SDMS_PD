                        </div>
                    </div>
                }
            }
            @Html.GetVSMWidgetsEnd()
        }
        using (r.GridColumn(6).Begin())
        {
            @Html.GetVSMWidgetsBegin(Translation.MIS.MISS01P001.ISSUE_DATETIME)
            using (r.GridColumn(12).Begin())
            {
                using (r.GridColumn(12).Begin())
                {
                    <div class="form-group">
                        <label class="control-label col-md-4">@Translation.MIS.MISS01P001.TARGET_DATE :</label>
                        <div class="col-md-8">
                            <label class="control-label">@Model.TARGET_DATE</label>
                        </div>
                    </div>
                }
                using (r.GridColumn(12).Begin())
