                        <div class="col-md-8">
                            <label class="control-label">@Model.ISSUE_TYPE</label>
                        </div>
                    </div>
                }
            }
            using (var r = Html.Bootstrap().GridRow().Begin())
            {
                using (r.GridColumn(6).Begin())
                {
                    @form.TextAreaFor(a => a.DETAIL, 4).SetReadonly()

                }
                using (r.GridColumn(6).Begin())
                {

                    <div class="form-group">
                        <label class="control-label col-md-4">@Translation.MIS.MISS01P001.RESPONSE_BY :</label>
                        <div class="col-md-8">
                            <label class="control-label">@Model.RESPONSE_BY</label>
                        </div>
                    </div>
                }
            }
            using (var r = Html.Bootstrap().GridRow().Begin())
            {
                using (r.GridColumn(6).Begin())
                {
